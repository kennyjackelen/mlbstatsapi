{
  "extends": "tslint:recommended",
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "no-namespace": false,
    "arrow-parens": false,
    "prefer-const": false,
    "object-literal-shorthand": false,
    "ordered-imports": false,
    "max-line-length": {
      "options": [
        120
      ]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": {
      "severity": "warning",
      "options": [
        "debug",
        "info",
        "log",
        "time",
        "timeEnd",
        "trace"
      ]
    }
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        120
      ]
    }
  },
  "cliOptions": {
    "exclude": [
      "node_modules"
    ]
  }
}